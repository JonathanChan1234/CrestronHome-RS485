<?xml version="1.0" encoding="UTF-8" ?>
<uidefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://prd-use-rad-assets.azurewebsites.net/ExtensionsSchemaDefinition_v1_0.xsd">
    <version ver="1.0"/>
    <tile icon="{LiftIcon}" status="{LiftStatusLabel}" action="command:CallLiftOne" navigation="show:MainPage" showinhomepage="#false" showinroompage="#true"/>
    <layouts>
        <layout id="MainPage" title="{MainPageLabel}">
            <controls>
                <button id="callLiftOneId" label="^CallLiftOne" icon="#icGateOpenDisabled" action="command:CallLiftOne"/>
                <button id="callLiftTwoId" label="^CallLiftTwo" icon="#icGateOpenDisabled" action="command:CallLiftTwo"/>
                <button id="callLiftThreeId" label="^CallLiftThree" icon="#icGateOpenDisabled" action="command:CallLiftThree"/>
            </controls>
        </layout>
    </layouts>
    <alerts/>
</uidefinition>